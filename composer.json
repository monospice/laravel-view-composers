{
    "name": "monospice/laravel-view-composers",
    "description": "An intuitive abstraction for organizing Laravel View Composers and View Creators",
    "keywords": ["laravel", "view", "composer", "creator"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Cy Rossignol",
            "email": "cy@rossignols.me"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "illuminate/support": ">=4.0",
        "monospice/spicy-identifiers": "1.0.x-dev"
    },
    "require-dev": {
        "phpspec/phpspec": "~2.3",
        "mockery/mockery": "0.9.*"
    },
    "autoload": {
        "psr-4": {
            "Monospice\\LaravelViewComposers\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Monospice\\LaravelViewComposers\\Test\\": "tests/"
         }
    }
}
